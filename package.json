{
  "name": "fyn",
  "version": "0.2.43",
  "description": "A fast node package manager for better productivity and efficiency",
  "preferGlobal": true,
  "main": "./bin/fyn.js",
  "scripts": {
    "test": "clap -x create-tgz electrode/test",
    "coverage": "clap -x create-tgz check",
    "ci": "clap -n -x bundle coverage && cat coverage/lcov.info | coveralls",
    "prepublishOnly": "echo Do not run npm publish directly.  Run clap release. && exit 1"
  },
  "bin": {
    "fyn": "./bin/fyn.js",
    "fun": "./bin/fun.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "keywords": [
    "node",
    "node_modules",
    "module",
    "modules",
    "fast",
    "package",
    "manager",
    "enhanced",
    "local",
    "better",
    "improve",
    "npm",
    "link",
    "development",
    "workflow",
    "productivity",
    "efficient",
    "disk",
    "space",
    "usage"
  ],
  "author": "Joel Chen <joel123@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/fyn.git"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "cacache": "jchip/cacache",
    "chalk": "^4.0.0",
    "ci-info": "^2.0.0",
    "es6-promisify": "./es6-promisify",
    "flat-module": "^1.0.0",
    "ini": "^1.3.5",
    "item-queue": "^1.0.4",
    "lockfile": "^1.0.4",
    "lodash": "^4.17.10",
    "make-fetch-happen": "jchip/make-fetch-happen",
    "mississippi": "^3.0.0",
    "mkdirp": "^0.5.1",
    "nix-clap": "^1.3.11",
    "npm-lifecycle": "^2.1.0",
    "npm-packlist": "^1.1.11",
    "npm-registry-fetch": "jchip/npm-registry-fetch",
    "npmlog": "^4.1.2",
    "opfs": "^1.1.1",
    "optional-require": "^1.0.0",
    "pacote": "jchip/pacote",
    "pkg-preper": "^0.1.0",
    "require-at": "^1.0.4",
    "rimraf": "^2.6.2",
    "semver": "^7.3.0",
    "tar": "^6.0.0",
    "visual-exec": "^0.1.7",
    "visual-logger": "^1.1.2",
    "xaa": "^1.3.0",
    "xenv-config": "^1.3.0",
    "xsh": "^0.4.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.3",
    "electrode-archetype-njs-module-dev": "^3.0.0",
    "electrode-server": "^3.0.3",
    "mock-require": "^2.0.2",
    "prettier": "^1.19.1",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "xstdout": "^0.1.1"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "coverage",
      "*clap.js",
      "gulpfile.js",
      "dist",
      "test",
      "xout",
      "tmp",
      "flat-module.js",
      "webpack.config.js",
      "bin",
      "dist",
      "stubs",
      "*/pkg-bin-linker-win32.js",
      "es6-promisify",
      "cli/config"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "opfs": {
    "mkdirp": true,
    "rimraf": true,
    "bluebird": true
  },
  "prettier": {
    "printWidth": 100
  }
}
